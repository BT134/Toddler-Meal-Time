{
  "name": "toddler-meal-times",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^2.0.1",
    "@chakra-ui/icon": "^1.1.13",
    "@chakra-ui/react": "^1.6.10",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "bcrypt": "^5.0.1",
    "concurrently": "^5.3.0",
    "framer-motion": "^4.1.17"
